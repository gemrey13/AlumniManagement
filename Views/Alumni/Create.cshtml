@model AlumniManagement.Entities.Alumnus

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Alumnus</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="FirstName" class="control-label"></label>
                <input asp-for="FirstName" class="form-control" />
                <span asp-validation-for="FirstName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LastName" class="control-label"></label>
                <input asp-for="LastName" class="form-control" />
                <span asp-validation-for="LastName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Username" class="control-label"></label>
                <input asp-for="Username" class="form-control" />
                <span asp-validation-for="Username" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Password" class="control-label"></label>
                <input asp-for="Password" class="form-control" />
                <span asp-validation-for="Password" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CourseGraduated" class="control-label"></label>
                <input asp-for="CourseGraduated" class="form-control" />
                <span asp-validation-for="CourseGraduated" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="YearGraduated" class="control-label"></label>
                <input asp-for="YearGraduated" class="form-control" />
                <span asp-validation-for="YearGraduated" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="WorkingStatus" /> @Html.DisplayNameFor(model => model.WorkingStatus)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="CurrentWork" class="control-label"></label>
                <input asp-for="CurrentWork" class="form-control" />
                <span asp-validation-for="CurrentWork" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

